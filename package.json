{
  "name": "twitterclone",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "parcel src/index.html ",
    "start": "npm run clear && npm run dev",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "clear": "rm -rf dist .parcel-cache docs",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "build": "npm run clear && NODE_ENV=production PUBLIC_URL=TwitterClone parcel build src/index.html --public-url ./ --dist-dir docs"
  },
  "keywords": [],
  "author": "apurv kumar",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "@parcel/transformer-svg-react": "^2.0.0-nightly.1739",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.3.2"
  },
  "browserslist": {
    "chrome": "58",
    "ie": "8"
  }
}
